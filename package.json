{
  "name": "gulp-wp-theme",
  "version": "0.1.0",
  "description": "WordPress Theme Project Templates for Gulp ",
  "repository": {
    "type": "git",
    "url": "https://github.com/whatwedo/gulp-wp-theme.git"
  },
  "browser": {
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "hbsfy"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "browserify-shim": {
    "jQuery": "global:jQuery"
  },
  "devDependencies": {
    "browser-sync": "~1.3.6",
    "browserify": "~5.10.1",
    "browserify-shim": "~3.7.0",
    "coffeeify": "~0.7.0",
    "gulp": "^3.8.7",
    "gulp-changed": "^0.4.1",
    "gulp-imagemin": "^0.6.2",
    "gulp-notify": "^1.4.2",
    "gulp-ruby-sass": "^0.7.1",
    "gulp-util": "^3.0.0",
    "hbsfy": "~2.0.0",
    "pretty-hrtime": "~0.2.1",
    "require-dir": "^0.1.0",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~1.0.2"
    "gulp-util": "~2.2.14",
    "pretty-hrtime": "~0.2.1",
    "yargs": "~1.2.6",
    "gulp-sass": "~0.7.2",
    "gulp-autoprefixer": "*",
    "gulp-minify-css": "*",
    "tiny-lr": "*",
    "gulp-livereload": "*",
    "gulp-plumber": "*",
    "gulp-if": "~1.2.4"
  },
  "dependencies": {
  }
}
