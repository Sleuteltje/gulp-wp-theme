{
  "name": "gulp-wp-theme",
  "version": "0.2.6",
  "description": "WordPress Theme Project Templates for Gulp ",
  "repository": {
    "type": "git",
    "url": "https://github.com/whatwedo/gulp-wp-theme.git"
  },
  "browser": {
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "browserify-shim": {
    "jQuery": "global:jQuery"
  },
  "devDependencies": {
    "browser-sync": "~1.3.6",
    "browserify": "~5.10.1",
    "browserify-shim": "~3.7.0",
    "glob-stream": "^3.1.15",
    "gulp": "^3.8.7",
    "gulp-autoprefixer": "*",
    "gulp-changed": "^0.4.1",
    "gulp-if": "~1.2.4",
    "gulp-imagemin": "^0.6.2",
    "gulp-minify-css": "^0.3.11",
    "gulp-notify": "^1.4.2",
    "gulp-plumber": "*",
    "gulp-sourcemaps": "^1.2.4",
    "gulp-stylus": "^2.0.0",
    "gulp-substituter": "^0.3.0",
    "gulp-util": "^3.0.0",
    "hbsfy": "~2.0.0",
    "pretty-hrtime": "~0.2.1",
    "require-dir": "^0.1.0",
    "through2": "^0.6.3",
